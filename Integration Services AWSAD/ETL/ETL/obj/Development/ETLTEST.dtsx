                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">Select * From (
Select Distinct 
	DueDate			AS Data,
	DAY(DueDate)	AS Dia,
	MONTH(DueDate)	AS Mes,
	YEAR(DueDate)	AS Ano,
		DATEPART(QUARTER, DueDate) AS Trimestre,
	CASE
		WHEN DATEPART(QUARTER, DueDate) &gt; 2 THEN 2
		ELSE 1
	END AS Semestre,
	DATENAME(WEEKDAY, DueDate) AS NomeDiaSemana,
	DATENAME(MONTH, DueDate) AS NomeMes 
From	
	AdventureWorks2014.Purchasing.PurchaseOrderDetail AS POD
UNION
Select Distinct 
	DueDate			AS Data,
	DAY(DueDate)	AS Dia,
	MONTH(DueDate)	AS Mes,
	YEAR(DueDate)	AS Ano,
		DATEPART(QUARTER, DueDate) AS Trimestre,
	CASE
		WHEN DATEPART(QUARTER, DueDate) &gt; 2 THEN 2
		ELSE 1
	END AS Semestre,
	DATENAME(WEEKDAY, DueDate) AS NomeDiaSemana,
	DATENAME(MONTH, DueDate) AS NomeMes 
From
	AdventureWorks2014.Sales.SalesOrderHeader AS SOH
UNION
Select Distinct 
	DueDate		AS Data,
	DAY(DueDate)	AS Dia,
	MONTH(DueDate)	AS Mes,
	YEAR(DueDate)	AS Ano,
		DATEPART(QUARTER, DueDate) AS Trimestre,
	CASE
		WHEN DATEPART(QUARTER, DueDate) &gt; 2 THEN 2
		ELSE 1
	END AS Semestre,
	DATENAME(WEEKDAY, DueDate) AS NomeDiaSemana,
	DATENAME(MONTH, DueDate) AS NomeMes 
From	
	AdventureWorks2014.Production.WorkOrder) AS WO</property>
